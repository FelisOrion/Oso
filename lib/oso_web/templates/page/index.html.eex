	function checkIfThereAreWhiteSpace(s, maxChars){
		var index = s.indexOf(" ");

		if(index == -1 && s.length > maxChars)
			return true;

		if(index != -1 && s.length > maxChars)
			return checkIfThereAreWhiteSpace(s.substr(index+1, s.length), maxChars);

		return false;
	}

	//recursive function to insert break chars every max char
	function insertBreakChar(s){

		var max = 5;
		var firstPart = s.substr(0,max) + "&#8203;";
		var secondPart;

		if(s.length - firstPart.length > max){
			secondPart = insertBreakChar(s.substr(max, s.length));
			}
		else{
			secondPart = s.substr(max, s.length);
			}

		return firstPart + secondPart;
	}
</script>
