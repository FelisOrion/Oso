 <script type="text/javascript">
	 (function(){

		function detectmob() {
			 if( navigator.userAgent.match(/Android/i)
			 || navigator.userAgent.match(/webOS/i)
			 || navigator.userAgent.match(/iPhone/i)
			 || navigator.userAgent.match(/iPad/i)
			 || navigator.userAgent.match(/iPod/i)
			 || navigator.userAgent.match(/BlackBerry/i)
			 || navigator.userAgent.match(/Windows Phone/i)
			 ){
				return true;
			  }
			 else {
				return false;
			  }
		}

		function removeMute(){
			if (detectmob() == false){
				$('#video_1').removeAttr('muted');
			}
		}

		removeMute();

	 	var playFirstTime = function(){
			var player = this;
			var videoFile = $(player.el()).find('source').first().attr('src');
			if (videoFile === undefined){
				videoFile = true;
			}

			if (window.localStorage && videoFile != window.localStorage.homeVideoPlayed){

				var showBar = function(){
					$('.vjs-control-bar').show();
				};

				$('#video_1').one('touchstart', function(){
					$('video',this).prop('muted',false);
					showBar();
				});
				$('.vjs-control-bar').hide();
				this.play();

				this.one('ended', function(){
					showBar();
				});

				window.localStorage.homeVideoPlayed = videoFile;
			}

	 	}

	 	if (videojs){
	 		videojs('#video_1').ready(playFirstTime);
	 	}

	 })();
 </script>


